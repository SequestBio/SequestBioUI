{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "SequestBioApp": "Information",
      "RiskCalculator": "Information",
      "SequestBioAI": "Information"
    }
  },
  "SequestBio": {
    "Application": {
      "Environment": "Production"
    },
    "RiskCalculation": {
      "EnableMockData": false,
      "MaxProcessingTimeSeconds": 30
    },
    "UI": {
      "ShowDebugInfo": false,
      "EnableAnimations": true
    },
    "Security": {
      "RequireHttps": true,
      "EnableCors": false
    },
    "Performance": {
      "EnableCaching": true,
      "CacheExpirationMinutes": 60,
      "MaxConcurrentCalculations": 3
    }
  }
} 