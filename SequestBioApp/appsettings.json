{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "SequestBioApp": "Information",
      "RiskCalculator": "Information",
      "SequestBioAI": "Information"
    }
  },
  "AllowedHosts": "*",
  "SequestBio": {
    "Application": {
      "Name": "SequestBio Risk Assessment Platform",
      "Version": "1.0.0",
      "Environment": "Development"
    },
    "FileUpload": {
      "MaxFileSizeBytes": 10485760,
      "AllowedExtensions": [".tsv", ".txt"],
      "UploadTimeoutSeconds": 300
    },
    "RiskCalculation": {
      "DefaultConfidenceThreshold": 70,
      "MaxProcessingTimeSeconds": 60,
      "EnableMockData": true,
      "GeneExpressionThreshold": 1.0
    },
    "UI": {
      "Theme": "default",
      "ShowDebugInfo": true,
      "EnableAnimations": true,
      "CardRefreshIntervalSeconds": 30
    },
    "Security": {
      "RequireHttps": false,
      "EnableCors": true,
      "AllowedOrigins": ["https://localhost:7000", "http://localhost:5000"]
    },
    "Performance": {
      "EnableCaching": true,
      "CacheExpirationMinutes": 30,
      "MaxConcurrentCalculations": 5
    }
  }
}
