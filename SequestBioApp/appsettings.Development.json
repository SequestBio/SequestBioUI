{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "SequestBioApp": "Debug",
      "RiskCalculator": "Debug",
      "SequestBioAI": "Debug"
    }
  },
  "SequestBio": {
    "Application": {
      "Environment": "Development"
    },
    "RiskCalculation": {
      "EnableMockData": true,
      "MaxProcessingTimeSeconds": 120
    },
    "UI": {
      "ShowDebugInfo": true,
      "EnableAnimations": true
    },
    "Security": {
      "RequireHttps": false,
      "EnableCors": true
    },
    "Performance": {
      "EnableCaching": false,
      "MaxConcurrentCalculations": 10
    }
  }
}
